{
    "name": "matejch/yii2-parcel",
    "description": "Module to work with slovak parcel service",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","parcel","soap","api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Matej Chalachan",
            "email": "matejCH.work@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.42",
        "matejch/yii2-xmlhelper": "1.0.0",
        "ext-curl": "*",
        "ext-soap": "*",
        "ext-openssl": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "matejch\\parcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "matejch\\parcel\\tests\\": "tests/"
        }
    }
}
